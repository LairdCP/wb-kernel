/dts-v1/;
#include "at91-ig60-som60.dtsi"
#include "at91-ig60_somc.dtsi"

/ {
	model = "Laird IG60";
	compatible = "laird,ig60", "laird,som60", "atmel,sama5d36", "atmel,sama5d3", "atmel,sama5";

	chosen {
		stdout-path = &dbgu;
		tick-timer = &pit;
	};
};

