/dts-v1/;

/* SOM60 module definition */
#include "at91-som60.dtsi"

/* DVK MFG board definition */
#include "at91-dvk_su60_somc.dtsi"

/* On-board 60 series radio disabled */
&mmc1 {
	status = "disabled";
};

/* LWB requires minimum 150ms after power-up until first access */
&reg_mmc0_en {
	startup-delay-us = <150000>;
};

/ {
	model = "Laird LWB MFG";
	compatible = "laird,lwb-som60", "laird,som60", "atmel,sama5d36", "atmel,sama5d3", "atmel,sama5";
};
